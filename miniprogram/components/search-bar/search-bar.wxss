.search-container {
    background-color: #4c1f24;
    width: 100%;
    height: 100rpx;
    display: flex;
    align-items: center;
}
.input-box {
    flex: 1;
    background-color: antiquewhite;
    height:60rpx;
    border-radius: 30rpx;
    margin-left: 20rpx;
    display: flex;
    align-items: center;
}
.search-btn {
    margin: 10rpx 20rpx;
    background-color: antiquewhite;
}
.icon-search {
    margin: 0 10rpx;
}
.icon-cancel {
    margin: 0 10rpx;
}
.input-bar {
    flex:1;
    font-size: 32rpx;
    line-height: 32rpx;
    color: #666;
}