.image-control {
    display: flex;
    padding: 10rpx;
}
.image-wraper {
    width: 100%;
    height: 280rpx;
    background-color: #7a7374;
    overflow: hidden;
}
.image {
    width: 100%;
}
.resume input{
    background-color: #b6b3b4;
    line-height: 50rpx;
    margin: 10rpx 20rpx;
    border-radius: 10rpx;
    padding: 20rpx;
}
.content {
    margin: 10rpx 20rpx;
}
.content textarea {
    background-color: #b6b3b4;
    border-radius: 10rpx;
    padding: 20rpx;
    overflow: auto;
    word-break: break-all;
}
